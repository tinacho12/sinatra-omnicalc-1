<form action = "/random/results">

<div>
<label for="random_minimum">Minimum</label>
<input id="random_minimum" type="text" name="min" placeholder="E.g. 1.5">
</div>

<div>
<label for="random_maximum">Maximum</label>
<input id="random_maximum" type="text" name="max" placeholder="E.g. 4.5">
</div>

<button>Pick random number</button>
</form>
